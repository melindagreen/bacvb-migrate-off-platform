{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "kraken-core/responsive-spacer",
	"version": "3.0.0",
	"title": "[Kraken] Responsive Spacer",
	"description": "A responsive spacer",
	"icon": "editor-expand",
	"category": "kraken-core",
	"attributes": {
		"heightDesktop": {
			"type": "string",
			"default": "4rem"
		},
		"heightTablet": {
			"type": "string",
			"default": "4rem"
		},
		"heightMobile": {
			"type": "string",
			"default": "3rem"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
