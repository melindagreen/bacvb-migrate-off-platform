{
  "apiVersion": 3,
  "name": "madden-theme/search-toggle",
  "title": "[MM] Search Toggle",
  "description": "Button that toggles the search form",
  "icon": "search",
  "category": "text",
  "textdomain": "madden-theme",
  "version": "1.0.0",
  "attributes": {
    "mobileBreakpoint": {
      "type": "string",
      "default": "lg"
    },
    "searchLabel": {
      "type": "string",
      "default": "Toggle site search"
    },
    "searchTarget": {
      "type": "string",
      "default": ".wp-block-search"
    },
    "searchIconColor": {
			"type": "string",
			"default": "black"
    },
    "searchIconActiveColor": {
			"type": "string",
			"default": "red"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"style": "file:./style-view.css",
  "render": "file:./render.php"
}