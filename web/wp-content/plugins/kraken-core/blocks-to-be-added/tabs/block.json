{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "madden-theme/tabs",
	"title": "[MM] Tabs",
	"description": "A container for tabbed content.",
	"category": "madden-media",
	"icon": "index-card",
	"attributes": {
		"tabBackgroundColor": { "type": "string" },
		"tabTextColor": { "type": "string" },
		"tabBackgroundHoverColor": { "type": "string" },
		"tabTextHoverColor": { "type": "string" },
		"tabBackgroundActiveColor": { "type": "string" },
		"tabTextActiveColor": { "type": "string" }
	},
	"render": "file:./render.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-view.css",
	"viewScript": "file:./view.js",
	"styles": [
		{
			"name": "default", 
			"label": "Default", 
			"isDefault": true
		},
		{
			"name": "seasonal-content", 
			"label": "Seasonal Content" 
		}
	]
}