{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "madden-theme/single-tab",
	"title": "Tab Panel",
	"description": "A single tab panel for use within the Tabs block.",
	"category": "madden-media",
	"parent": [ "madden-theme/tabs" ],
	"icon": "text-page",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Tab Title"
		},
		"tabBackgroundColor": { "type": "string" },
		"tabTextColor": { "type": "string" },
		"tabBackgroundHoverColor": { "type": "string" },
		"tabTextHoverColor": { "type": "string" },
		"tabBackgroundActiveColor": { "type": "string" },
		"tabTextActiveColor": { "type": "string" }
	},
	"render": "file:./render.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}