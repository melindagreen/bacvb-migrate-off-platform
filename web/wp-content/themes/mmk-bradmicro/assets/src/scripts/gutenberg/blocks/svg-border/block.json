{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "madden-theme/svg-border",
	"version": "1.0.0b",
	"title": "[MM] SVG Border",
	"description": "Displays a svg border",
	"icon": "list-view",
	"category": "madden-media",
	"render": "file:./render.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"style": "file:./style-view.css",
	"supports": {
		"anchor": true,
    "align": [ "wide", "full" ],
    "color": {
      "text": true,
      "background": true
    },
		"spacing": {
			"margin": true
		}
	},
	"attributes": {
    "secondaryColor": {
			"type": "string",
			"default": "red"
		},
		"borderStyle": {
			"type": "string",
			"default": ""
		},
		"flipped": {
			"type": "boolean",
			"default": false
		},
    "flippedVertical": {
			"type": "boolean",
			"default": false
		},
    "minWidth": {
			"type": "string",
			"default": false
		}
	}
}
