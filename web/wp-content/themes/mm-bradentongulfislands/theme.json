{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "border": {
      "radius": false,
      "color": false,
      "style": false,
      "width": false
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true,
      "units": [
        "em",
        "rem",
        "vh",
        "vw",
        "%",
        "px"
      ]
    },
    "blocks": {
      "core/image": {
        "lightbox": {
          "allowEditing": true
        }
      }
    },
    "color": {
      "custom": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Bradenton Teal",
          "slug": "bradenton-teal",
          "color": "#2B7B7C"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#FFFFFF"
        },
        {
          "name": "Soft Black",
          "slug": "soft-black",
          "color": "#414141"
        },
        {
          "name": "Warm Gray",
          "slug": "warm-gray",
          "color": "#AFA8A0"
        },
        {
          "name": "Light Gray",
          "slug": "light-gray",
          "color": "#eeeeee"
        },
        {
          "name": "Sky Blue",
          "slug": "sky-blue",
          "color": "#B0D9E6"
        },
        {
          "name": "Sea Blue",
          "slug": "sea-blue",
          "color": "#82CAE2"
        },
        {
          "name": "Turquoise",
          "slug": "turquoise",
          "color": "#77C3C6"
        },
        {
          "name": "Bright Blue",
          "slug": "bright-blue",
          "color": "#037BBF"
        },
        {
          "name": "Purple",
          "slug": "purple",
          "color": "#4A6BAE"
        },
        {
          "name": "Dark Purple",
          "slug": "dk-purple",
          "color": "#5F769B"
        },
        {
          "name": "Dark Blue",
          "slug": "dk-blue",
          "color": "#014067"
        },
        {
          "name": "Rust",
          "slug": "rust",
          "color": "#C96A39"
        },
        {
          "name": "Yellow Tan",
          "slug": "yellow-tan",
          "color": "#F4EEE1"
        }
      ],
      "customGradient": true,
      "defaultGradients": false,
      "gradients": []
    },
    "typography": {
      "customFontSize": false,
      "letterSpacing": false,
      "textDecoration": false,
      "fontSizes": [
        {
          "slug": "small",
          "size": "1rem",
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "1.5rem",
          "name": "Normal"
        },
        {
          "slug": "medium",
          "size": "2rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "clamp(2.15rem, 3vw, 3rem)",
          "name": "Large"
        },
        {
          "slug": "xlarge",
          "size": "clamp(2.35rem, 5vw, 5rem)",
          "name": "X Large"
        }
      ],
        "fontFamilies": [
          {
            "fontFamily": "'Lifehack', sans-serif",
            "slug": "title",
            "name": "Title Font"
          },
          {
            "fontFamily": "'Melburch', sans-serif",
            "slug": "header",
            "name": "Header Font"
          },
          {
            "fontFamily": "'Open Sans', sans-serif",
            "slug": "body",
            "name": "Body Font"
          }
        ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)"
    }
  }
}