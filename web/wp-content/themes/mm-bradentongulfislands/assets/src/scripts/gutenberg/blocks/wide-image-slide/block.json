{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mm-bradentongulfislands/wide-image-slide",
  "version": "1.0.1",
  "title": "Image Slide",
  "description": "Image slide for parent Wide Carousel",
  "icon": "format-image",
  "category": "layout",
  "parent": [
    "mm-bradentongulfislands/wideslideshow"
  ],
  "supports": {
    "align": true
  },
  "attributes": {
    "mediaId": {
      "type": "number",
      "default": 0
    },
    "mediaUrl": {
      "type": "array",
      "default": [],
      "selector": "img",
      "attribute": "src"
    },
    "mediaAlt": {
      "type": "string",
      "default": "",
      "selector": "img",
      "attribute": "alt"
    },
    "mediaPhotoCredit": {
      "type": "string",
      "default": ""
    },
    "imagePosition": {
      "type": "string",
      "default": "center"
    },
    "logoId": {
      "type": "number",
      "default": 0
    },
    "logoUrl": {
      "type": "array",
      "default": [],
      "selector": "img",
      "attribute": "src"
    },
    "logoAlt": {
      "type": "string",
      "default": "",
      "selector": "img",
      "attribute": "alt"
    },
    "title": {
      "type": "string",
      "default": "",
      "selector": ".infoblock__item"
    },
    "info": {
      "type": "string",
      "default": "",
      "selector": ".infoblock__item"
    },
    "buttonUrl": {
      "type": "string",
      "default": "",
      "selector": "a",
      "attribute": "href"
    },
    "buttonText": {
      "type": "string",
      "default": "Learn More",
      "selector": "a",
      "attribute": "href"
    },
    "isLightbox": {
      "type": "boolean",
      "default": false
    },
    "lightboxTitle": {
      "type": "string",
      "default": ""
    },
    "lightboxSubtitle": {
      "type": "string",
      "default": ""
    },
    "lightboxButtonText": {
      "type": "string",
      "default": "Visit Website"
    },
    "lightboxEmbedSrc": {
      "type": "string",
      "default": ""
    },
    "bannerTitle": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./style-index.css"
}
