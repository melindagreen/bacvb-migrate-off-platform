{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mm-bradentongulfislands/cta-bubble",
  "version": "1.0.1",
  "title": "CTA Bubble",
  "description": "bubble cta",
  "icon": "share-alt",
  "category": "layout",
  "attributes": {
    "title": {
      "type": "string",
      "default": "",
      "source": "html",
      "selector": "h2"
    },
    "description": {
      "type": "string",
      "default": "",
      "source": "html",
      "selector": "p"
    },
    "ctaText": {
      "type": "string",
      "default": "",
      "source": "html",
      "selector": "button"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./style-index.css",
  "viewScript": "file:./front.js"
}
