{
  "name": "example-dynamic",
  "dynamic": true,
  "scripts": [
    "front.js"
  ],
  "render_callback": "MaddenNino\\Blocks\\ExampleDynamic\\render_block",
  "settings": {
    "title": "Dynamic Block Example",
    "description": "An example block",
    "icon": "smiley",
    "category": "layout",
    "attributes": {
      "exampleImageIds": {
        "type": "array",
        "default": [],
        "items": {
          "type": "number"
        }
      },
      "exampleText": {
        "type": "string",
        "default": "Hello, Madden!",
        "selector": ".exampleText"
      },
      "exampleColor": {
        "type": "string",
        "default": "#CC0000"
      },
      "exampleCatFilter": {
        "type": "array",
        "default": [],
        "items": {
          "type": "string"
        }
      },
      "mode": {
        "type": "string",
        "default": "preview"
      }
    }
  }
}