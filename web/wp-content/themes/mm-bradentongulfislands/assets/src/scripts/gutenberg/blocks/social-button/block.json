{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mm-bradentongulfislands/social-button",
  "version": "1.0.0",
  "title": "Social Button",
  "description": "social button",
  "icon": "share-alt",
  "category": "layout",
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": "",
      "source": "html",
      "selector": "h3.contents-title"
    },
    "mode": {
      "type": "string",
      "default": "preview"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./style-index.css",
  "viewScript": "file:./front.js"
}
