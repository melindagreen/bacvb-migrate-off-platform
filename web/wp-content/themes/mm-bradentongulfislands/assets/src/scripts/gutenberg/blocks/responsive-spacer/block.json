{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mm-bradentongulfislands/responsive-spacer",
  "version": "1.0.0",
  "title": "Responsive Spacer",
  "description": "A responsive spacer",
  "icon": "list-view",
  "category": "madden-media",
  "attributes": {
    "heightDesktop": {
      "type": "number",
      "default": 4
    },
    "heightMobile": {
      "type": "number",
      "default": 4
    },
    "unit": {
      "type": "string",
      "default": "rem"
    },
    "spacerId": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./style-index.css"
}
