{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mm-bradentongulfislands/slider",
  "version": "1.0.1b",
  "title": "Slider",
  "description": "Slider using InnerBlocks & Swiper",
  "icon": "format-gallery",
  "category": "madden-media",
  "supports": {
    "anchor": true,
    "color": true,
    "editMode": true
  },
  "attributes": {
    "contentType": {
      "type": "string",
      "default": "custom"
    },
    "galleryImages": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object"
      }
    },
    "galleryMaxHeight": {
      "type": "string",
      "default": "100%"
    },
    "postType": {
      "type": "string",
      "default": "post"
    },
    "enableTaxFilter": {
      "type": "boolean",
      "default": false
    },
    "taxonomyFilter": {
      "type": "string",
      "default": "category"
    },
    "taxonomyTerms": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object"
      }
    },
    "numberOfPosts": {
      "type": "number",
      "default": 5
    },
    "manualPosts": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object"
      }
    },
    "cardStyle": {
      "type": "string",
      "default": "overlay"
    },
    "displayAdditionalContent": {
      "type": "boolean",
      "default": false
    },
    "displayExcerpt": {
      "type": "boolean",
      "default": false
    },
    "excerptLength": {
      "type": "number",
      "default": 12
    },
    "displayReadMore": {
      "type": "boolean",
      "default": false
    },
    "readMoreText": {
      "type": "string",
      "default": "Read More"
    },
    "enableCtaSlide": {
      "type": "boolean",
      "default": false
    },
    "ctaSlideTitle": {
      "type": "string",
      "default": ""
    },
    "ctaSlideExcerpt": {
      "type": "string",
      "default": ""
    },
    "ctaSlideImage": {
      "type": "number",
      "default": 0
    },
    "ctaSlideImageUrl": {
      "type": "string",
      "default": ""
    },
    "ctaSlideBtnText": {
      "type": "string",
      "default": ""
    },
    "ctaSlideBtnUrl": {
      "type": "object",
      "default": {}
    },
    "shuffleSlides": {
      "type": "boolean",
      "default": false
    },
    "enableMouseScroll": {
      "type": "boolean",
      "default": false
    },
    "enableAutoplay": {
      "type": "boolean",
      "default": false
    },
    "centeredSlides": {
      "type": "boolean",
      "default": false
    },
    "effect": {
      "type": "string",
      "default": "slide"
    },
    "loop": {
      "type": "boolean",
      "default": false
    },
    "freeMode": {
      "type": "boolean",
      "default": false
    },
    "sliderDirectionDesktop": {
      "type": "string",
      "default": "horizontal"
    },
    "sliderDirectionTablet": {
      "type": "string",
      "default": "horizontal"
    },
    "sliderDirectionMobile": {
      "type": "string",
      "default": "horizontal"
    },
    "enableArrowNavigation": {
      "type": "boolean",
      "default": false
    },
    "arrowsBelowSlider": {
      "type": "boolean",
      "default": false
    },
    "enablePagination": {
      "type": "boolean",
      "default": true
    },
    "enableScrollbar": {
      "type": "boolean",
      "default": false
    },
    "arrowColor": {
      "type": "object",
      "default": {}
    },
    "arrowBackgroundColor": {
      "type": "object",
      "default": {}
    },
    "dotColor": {
      "type": "object",
      "default": {}
    },
    "dotColorActive": {
      "type": "object",
      "default": {}
    },
    "scrollbarColor": {
      "type": "object",
      "default": {}
    },
    "enableGridRows": {
      "type": "boolean",
      "default": false
    },
    "gridRowsDesktop": {
      "type": "number",
      "default": 1
    },
    "gridRowsTablet": {
      "type": "number",
      "default": 1
    },
    "gridRowsMobile": {
      "type": "number",
      "default": 1
    },
    "enableSpaceBetween": {
      "type": "boolean",
      "default": false
    },
    "spaceBetweenDesktop": {
      "type": "number",
      "default": 10
    },
    "spaceBetweenTablet": {
      "type": "number",
      "default": 10
    },
    "spaceBetweenMobile": {
      "type": "number",
      "default": 10
    },
    "enableSlidesPerView": {
      "type": "boolean",
      "default": false
    },
    "enableSlidesPerViewAuto": {
      "type": "boolean",
      "default": false
    },
    "slidesPerViewDesktop": {
      "type": "number",
      "default": 4
    },
    "slidesPerViewTablet": {
      "type": "number",
      "default": 2
    },
    "slidesPerViewMobile": {
      "type": "number",
      "default": 1
    },
    "enableSlidesPerGroup": {
      "type": "boolean",
      "default": false
    },
    "enableSlidesPerGroupAuto": {
      "type": "boolean",
      "default": false
    },
    "slidesPerGroupDesktop": {
      "type": "number",
      "default": 4
    },
    "slidesPerGroupTablet": {
      "type": "number",
      "default": 2
    },
    "slidesPerGroupMobile": {
      "type": "number",
      "default": 1
    },
    "mode": {
      "type": "string",
      "default": "edit"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewStyle": "file:./style-index.css",
  "viewScript": "file:./front.js",
  "render": "file:./render.php"
}
