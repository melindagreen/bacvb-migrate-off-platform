{
    "name": "listings-grid",
    "dynamic": true,
    "scripts": [
      "swiper.js",
      "front.js"
    ],
    "script_extra_dependencies": [
      "jquery-ui-datepicker"
    ],
    "render_callback": "MaddenNino\\Blocks\\ListingsGrid\\render_block",
    "settings": {
      "title": "Listings Grid",
      "description": "A grid/map view of blog posts, events or listings",
      "icon": "store",
      "category": "widgets",
      "attributes": {
        "listingsTitle": {
          "type": "string",
          "default": "Listings"
        },
        "postType": {
          "type": "string",
          "default": "listing"
        },
        "listingsPerPage": {
          "type": "number",
          "default": 12
        },
        "defaultThumb": {
          "type": "string",
          "default": "/wp-content/uploads/pexels-miriam-fischer-2645245-scaled.jpg"
        },
        "preFilterCat": {
          "type": "string",
          "default": "none"
        },
        "filterType": {
          "type": "string",
          "default": "categories"
        },
        "mode": {
          "type": "string",
          "default": "preview"
        }
      }
    }
  }