{
	"name": "maddenmedia/bacvb-migrate",
	"description": "WordPress Composer-managed multisite installation. Manages WordPress core, plugins, and themes.",
	"type": "project",
	"keywords": [
		"wordpress",
		"blog",
		"cms",
		"template"
	],
	"homepage": "https://github.com/melindagreen/bacvb-migrate-off-platform",
	"license": "GPL-2.0-or-later",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"davidbarratt/custom-installer": "^1.1",
		"vlucas/phpdotenv": "^5.6",
		"wp-cli/wp-cli-bundle": "^2.5",
		"symfony/console": "v7.1.4",
		"wpackagist-plugin/404-to-301": "3.1.5",
		"wpackagist-plugin/better-search-replace": "1.4.10",
		"wpackagist-plugin/block-visibility": "3.7.1",
		"wpackagist-plugin/create-block-theme": "2.7.0",
		"wpackagist-plugin/cloudflare": "4.13.0",
		"wpackagist-plugin/duplicate-post": "^4.4",
		"wpackagist-plugin/enable-media-replace": "^4.1.6",
		"wpackagist-plugin/fareharbor": "^3.6",
		"wpackagist-plugin/leadin": "11.3.21",
		"wpackagist-plugin/post-duplicator": "2.48",
		"wpackagist-plugin/post-type-switcher": "^4.0.0",
		"wpackagist-plugin/redirection": "^5.5",
		"wpackagist-plugin/safe-svg": "^2.1",
		"wpackagist-plugin/seo-by-rank-math": "*",
		"wpackagist-plugin/tiny-compress-images": "3.6.4"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"discard-changes": true,
		"sort-packages": true,
		"allow-plugins": {
			"composer/installers": true,
			"ffraenz/private-composer-installer": true,
			"davidbarratt/custom-installer": true
		}
	},
	"extra": {
		"custom-installer": {
			"web/wp-content/themes/mm-nino-theme/assets/src/scripts/gutenberg/blocks/{$name}": [
				"type:mm-wordpress-block"
			]
		},
		"installer-paths": {
			"web/wp-content/plugins/{$name}": [
				"type:wordpress-plugin"
			],
			"web/wp-content/themes/{$name}": [
				"type:wordpress-theme"
			],
			"web/wp-content/mu-plugins/{$name}": [
				"type:wordpress-muplugin"
			]
		}
	},
	"scripts": {
		"blocks": "@script select-block",
		"script": ".composer/script",
		"script:permit": "x=.composer/script; chmod -R +x $x ${x}s",
		"post-install-cmd": [
			"@script:permit",
			"@script install-wp-local-config"
		],
		"post-update-cmd": [],
		"build": "@script build",
		"build:child": "@build build:child",
		"watch": "@watch:child",
		"watch:child": "@build watch:child",
		"start": "@watch",
		"dev": "@watch",
		"prod": [
			"@build:child"
		],
		"production": "@prod",
		"install:child": "@script install-child",
		"install:node": "@script install-node",
		"platform": "@script platform-settings",
		"reinstall:child": "@build reinstall:child",
		"require-mm": "@script require-mm",
		"theme:dir": "@script theme-dir",
		"check-updates": "@script check-updates",
		"update-packages": []
	}
}
